{
  "expo": {
    "name": "SuperBad",
    "slug": "SuperBad",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "versionCode": 7,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.parkhyuna.SuperBad"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "e10de8d8-7431-4133-b360-48dac6eea30f"
      }
    }
  }
}
